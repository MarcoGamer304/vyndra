{
  "name": "vyndra-js",
  "version": "1.0.13",
  "description": "Micro Node.js framework with routing, ORM, decorators, and automatic DI",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "vyndra": "./bin/vyndra.js"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist/**/*",
    "bin/templates.js"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/index.js"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "decorators",
    "framework",
    "DI",
    "routing",
    "ORM",
    "microservices"
  ],
  "author": "Marco Alonso Elizondo SolÃ­s",
  "license": "MIT",
  "dependencies": {
    "@types/pg": "^8.15.6",
    "amqplib": "^0.10.9",
    "bull": "^4.16.5",
    "comander": "^0.0.1-security",
    "commander": "^14.0.2",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "inquirer": "^12.10.0",
    "node-docker-api": "^1.1.22",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.8",
    "@types/node": "^24.9.1",
    "@types/reflect-metadata": "^0.0.5",
    "typescript": "^5.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MarcoGamer304/vyndrajs.git"
  },
  "engines": {
    "node": ">=18"
  }
}
